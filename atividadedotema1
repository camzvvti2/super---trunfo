#include <stdio.h>

char estado1;
char codigo1[4];
char cidade1[50];
int populacao1[50];
float area1;
float pontosturisticos1;
float pib1;

  int main(){

//entrada da primeira carta

printf("digite o estado: (ex: A-H) \n");
scanf("%c", &estado1);

printf("codigo da carta1(ex: A01, B03):\n");
scanf("%s", &codigo1);


printf("digite a cidade: \n");
scanf("%s",cidade1);


printf("digite a população: \n");
scanf("%d", &populacao1);

printf("digite os pontos turisticos1: \n");
scanf("%e", &pontosturisticos1);


printf("digite a area1: \n");
scanf("%f", &area1);

printf("digite o pib1; \n");
scanf("%f", &pib1);

//entrada da segunda carta2
char estado2;
char codigo2[4];
char nomecidade2[50];
int populacao2[50];
float area2;
float pontosturisticos2;
float pib2;


printf("digite o estado: (ex: A-H) \n");
scanf("%c", &estado2);

printf("codigo da carta2 (ex: A01, B03):\n");
scanf("%s", &codigo2);


printf("digite a cidade: \n");
scanf("%s", &nomecidade2);

printf("digite a população: \n");
scanf("%d", &populacao2);


